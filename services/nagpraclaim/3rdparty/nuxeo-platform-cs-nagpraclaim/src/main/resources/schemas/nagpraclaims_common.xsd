		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="nagpraClaimReceivedGroupList"> <!-- CORE: Date filed -->
		<xs:sequence>
			<xs:element name="nagpraClaimReceivedGroup" type="nagpraClaimReceivedGroup" minOccurs="0" maxOccurs="unbounded"/>
