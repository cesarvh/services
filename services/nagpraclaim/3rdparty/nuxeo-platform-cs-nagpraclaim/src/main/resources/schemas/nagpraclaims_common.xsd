			<xs:element name="nagpraClaimReceivedDate" type="xs:string"/> <!-- CORE: Date filed -->
			<xs:element name="nagpraClaimReceivedNote" type="xs:string"/> <!-- SUGGESTED CORE -->
		</xs:sequence>
	</xs:complexType>
	
</xs:schema>

