<?xml version="1.0"?>
<component name="org.collectionspace.nagpraclaimTenant3.coreTypes">
	<extension target="org.nuxeo.ecm.core.schema.TypeService" point="schema">
		<schema name="nagpraclaims_common" prefix="nagpraclaims_common" src="schemas/nagpraclaims_common.xsd"/>
		<schema name="nagpraclaims_anthropology" prefix="nagpraclaims_anthropology" src="schemas/nagpraclaims_anthropology.xsd"/>
		<schema name="nagpraclaims_pahma" prefix="nagpraclaims_pahma" src="schemas/nagpraclaims_pahma.xsd"/>
	</extension>
	<extension target="org.nuxeo.ecm.core.schema.TypeService" point="doctype">
	    <doctype name="NagpraClaim" extends="Document">
			<schema name="common"/>
			<schema name="dublincore"/>
			<schema name="collectionspace_core"/>
			<schema name="nagpraclaims_common"/>
	    	<schema name="nagpraclaims_anthropology"/>
	    	<schema name="nagpraclaims_pahma"/>
			<prefetch>
				<!--nagpraclaims_anthropology:nagpraClaimName  PREPARING FOR MOVE OF FIELD TO ANTHROPOLOGY SCHEMA-->
				nagpraclaims_common:nagpraClaimName
				nagpraclaims_common:nagpraClaimNumber
			</prefetch>
		</doctype>
	</extension>
</component>
